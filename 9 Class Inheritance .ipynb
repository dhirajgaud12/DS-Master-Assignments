    "# 3) Multiple inheritance is where a class inherits from more than one parent class\n",
    "\n",
    "class a:\n",
    "    def test(self):\n",
    "        print(\"Parent 1 class\")\n",
    "        \n",
    "class b:\n",
    "    def test1(self):\n",
    "        print(\"Parent 2 class\")\n",
    "        \n",
    "class c(a,b):   \n",
    "    def test3(self):\n",
    "        print(\"Child class\")\n",
    "        \n",
    "\n",
    "classc=c()\n",
    "classc.test()\n",
    "classc.test1()\n",
    "classc.test3()"
   ]
  },
